# Creates an error in winter when there is garden waste collection dates !
sensor:
  - platform: rest
    resource: !secret binday_server_resource
    name: Bin Day Garden
    value_template: "{{ value_json.Garden[0].collectionDate }}"
    json_attributes:
      - BinType
      - collectionDate
      - nextCollectionDate
    json_attributes_path: Garden[2]