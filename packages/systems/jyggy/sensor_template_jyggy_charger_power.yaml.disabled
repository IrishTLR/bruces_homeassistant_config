sensor:
  - platform: template
    sensors:
      jyggy_charger_power:
        friendly_name: "Charging Power"
        value_template: "{{ state_attr('sensor.jyggy_sensor','charger_voltage') | float  * state_attr('sensor.jyggy_sensor','charger_actual_current') | float }}"
        unit_of_measurement: "W"
        device_class: power